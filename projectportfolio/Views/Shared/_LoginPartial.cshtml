@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager

<ul class="navbar-nav">
@if (SignInManager.IsSignedIn(User))
{
    <li class="">
        <form  class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
            <button  type="submit" class="btn btn-color">Logga ut</button>
        </form>
    </li>
}
else
{
    <li class="">
        <a class="btn btn-color" asp-area="Identity" asp-page="/Account/Register">Registrera</a>
    </li>
    <li class="">
        <a class="btn btn-color" asp-area="Identity" asp-page="/Account/Login">Logga in</a>
    </li>
}
</ul>
